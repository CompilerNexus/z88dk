XTOKEN(Plus, "+")
XTOKEN(Minus, "-")
XTOKEN(Mult, "*")
XTOKEN(Power, "**")
XTOKEN(Div, "/")
XTOKEN(Mod, "%")
XTOKEN(Eq, "=")
XTOKEN(Ne, "!=")
XTOKEN(Lt, "<")
