XTOKEN(Floating, "")
XTOKEN(String, "")

XTOKEN(Backslash, "\\")
XTOKEN(Semicolon, ";")
XTOKEN(Dollar, "$")
XTOKEN(BinNot, "~")
XTOKEN(LogNot, "!")
XTOKEN(BinAnd, "&")
XTOKEN(LogAnd, "&&")
XTOKEN(BinOr, "|")
